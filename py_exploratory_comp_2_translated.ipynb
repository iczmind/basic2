   "source": [
    "x,y = np.loadtxt('xypoints.dat')\n",
    "plt.plot(x,y,'ro')\n",
    "plt.plot(x[ (x<-2) & (y>=-5) & (y<00) ], y[ (x<-2) & (y>=-5) & (y<-0) ], 'bo')\n",
    "r = np.sqrt((x-5)**2+y**2)\n",
    "plt.plot(x[r<5],y[r<5],'go')\n",
    "plt.axis('image');"
   ]

   "source": [
    "a = 4\n",
    "print(a < 4)\n",
    "print(a <= 4) # a lebih kecil atau sama dengan 4\n",
    "print(a == 4) # a sama dengan 4. Perhatikan penggunaan tanda sama dengan ganda pada pernyataan ini\n",
    "print(a >= 4) \n",
    "print(a >  4)\n",
    "print(a != 4) # a tidak sama dengan 4"
   ]
